	Issn = {07475632},
	Journal = {Computers in Human Behavior},
	Keywords = {Distraction-Conflict theory,Multitasking computer self-efficacy,Social media,Task performance,Technostress},
	Pages = {26--37},
	Publisher = {Elsevier Ltd},
	Title = {{Does personal social media usage affect efficiency and well-being?}},
	Volume = {46},
	Year = {2015},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.chb.2014.12.053}}

