keywords = {media diffusion,parental mediation,parenting support},
month = {may},
number = {1},
title = {{Guiding young children's internet use at home: Problems that parents experience in their parental mediation and the need for parenting support}},
url = {https://cyberpsychology.eu/article/view/4327/3384},
volume = {9},
year = {2015}
}
