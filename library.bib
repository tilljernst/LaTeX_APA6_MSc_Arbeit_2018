journal = {CHI 2008 Proc. SIGCHI Conf. Hum. Factors Comput. Syst.},
keywords = {interuptions,multi-tasking,work},
pages = {107--110},
pmid = {1062},
title = {{The Cost of Interrupted Work : More Speed and Stress}},
year = {2008}
}
