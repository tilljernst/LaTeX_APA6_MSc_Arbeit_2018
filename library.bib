@article{Livingstone2015,
	Abstract = {This editorial discusses the difference made by 'the digital' to children's experiences of risk. In Brindova et al.'s (2014) article in this issue, a large-scale correlational study sets up just the puzzle: watching television for more than 3 h per day is correlated with a host of adverse symptoms (headaches, irritability, and depression). The authors complicate the story by showing what many have suspected, namely a curvilinear relation between, in this case, amount of online communication and life satisfaction. To resolve the correlation/cause problem in a domain where one can hardly experiment with teenagers' lives, longitudinal research is helpful. The authors report that higher use of mobile phones and television viewing is associated, 1 year later, with greater levels of depression among teenagers. This seems to establish the case for media harm. But, being cautious, one should note that it is not clear whether the authors have investigated or controlled for the possible role of offline risks in teenagers' lives (classically, as a putative 'third cause' of the observed correlations). (PsycINFO Database Record (c) 2015 APA, all rights reserved)},
	Author = {Livingstone, Sonia},
	Doi = {10.1007/s00038-015-0661-3},
	File = {:Users/gratch/Library/Application Support/Mendeley Desktop/Downloaded/Livingstone - 2015 - What difference does `the digital' make to children's experiences of risk.pdf:pdf},
