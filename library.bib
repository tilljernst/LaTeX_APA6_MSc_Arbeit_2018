	File = {:Users/gratch/Google Drive/ZHAW{\_}Psy{\_}MSc/Z AF4 u AF5 - Masterarbeit/LiteraturRecherche/Costanza2007.pdf:pdf},
	Issn = {0921-8009},
	Journal = {Ecological Economics},
	Month = {mar},
	Number = {2-3},
	Pages = {267--276},
	Publisher = {Elsevier},
	Title = {{Quality of life: An approach integrating opportunities, human needs, and subjective well-being}},
	Volume = {61},
	Year = {2007},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/J.ECOLECON.2006.02.023}}

@article{Grau1999,
